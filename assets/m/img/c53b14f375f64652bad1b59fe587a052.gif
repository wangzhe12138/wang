<script type="text/html" id="recommendView">
    <%
    var srcStr = isScrollLoad ? 'init_src' : 'src';
    for(var i = 0,len=data.length; i < len; i++){
        var oItem = data[i],
            ydTpObj = oItem.ydTpObj,
            priceArr = oItem.dpicon && oItem.dpicon.icon==4 ? String(oItem.dpicon.p).split('.') : String(oItem.jp).split('.');
    %>
    <li class="<%=oItem.source == 0 ? 'ad_li' : ''%>" attr-tag="cnxhlink" data-href="<%=oItem.linkUrl%>" data-repurl="<%=oItem.repURL%>" data-exposalurl="<%=oItem.client_exposal_url%>" source="<%=oItem.source%>" sku="<%=oItem.sku%>" clk="<%=oItem.clk%>">
        <div class="cover">
            <img <%=srcStr%>="<%=JD.img.getImgUrl('//img14.360buyimg.com/n7/'+oItem.img)%>">
            <% if(ydTpObj){ %>
                <% if(ydTpObj.catType==1 && ydTpObj.url){%>
                <div class="mod_good_decoration type_rem" style="background-image: url(<%=ydTpObj.url%>);">
                <% }else{ %>
                <div class="mod_good_decoration">
                <% } %>
                <% if([1,3].indexOf(ydTpObj.catType)!=-1){%>
                <div class="mod_good_decoration_circle">
                    <div class="mod_good_decoration_circle_text"><%=ydTpObj.c.slice(0,4)%></div>
                    <div class="mod_good_decoration_circle_price">¥<%=ydTpObj.p%></div>
                    <div class="mod_good_decoration_circle_date"><%=ydTpObj.timeStr%></div>
                </div><% } %>
                <% if([1,2].indexOf(ydTpObj.catType)!=-1){%>
                    <div class="mod_good_decoration_belt" style="<%if(ydTpObj.url && ydTpObj.catType!=1){%> background-size: 100% 100%;background-image: url(<%=ydTpObj.url%>); <%}%> " >
                        <div class="mod_good_decoration_belt_text"><%=ydTpObj.content + (ydTpObj.catType==2 ? ("(" + ydTpObj.timeStr + ")"): "")%></div>
                    </div>
                <% } %>
                </div>
            <% } %>
            <% if(oItem.reserve == '1'){ %>
                <div class="mod_good_decoration" style="text-align:center; background: rgba(233,59,61,.7); color: #fff; line-height: 1.1rem;">
                    预约
                </div>
            <% } %>
        </div>
        <div class="info">
            <div class="name">
                <% if(oItem.prom==1 && data.dcImg){ %>
                    <i class="mod_tag"><img src="<%=JD.img.getImgUrl(data.dcImg)%>"></i>
                <% } %>
                <% if(oItem.prom==2){ %>
                    <i class="mod_tag"><img src="https://img11.360buyimg.com/jdphoto/s370x56_jfs/t23851/153/2584558289/12432/70445100/5b87ce90N007c6bab.jpg"></i>
                <% } %>
                <% if(oItem.iconImg){ %>
                <i class="mod_tag"><img src="<%=JD.img.getImgUrl(oItem.iconImg)%>"></i>
                <% }  %>
                <%=oItem.t%>
            </div>
            <div class="flex_wrap">
                <div class="price_info">
                    <% if(oItem.dpicon && oItem.dpicon.icon==4){ %><div class="price"><%=oItem.dpicon.count %>人拼</div><% } %>
                    <% if(oItem.jpnonshow > 0){ %>
                        <div class="price">¥<em> 待发布</em></div>
                    <% } else { %>
                        <div class="price">¥<em><%=priceArr[0]%></em><%= priceArr[1] ? ('.' + priceArr[1]): ''%></div>
                    <% } %>
                    <% if(oItem.paiconStr){ %>
                    <div class="mod_sign_tip bor "><span><%=oItem.paiconStr%></span></div>
                    <% } %>
                    <%  if(oItem.dpicon && oItem.dpicon.icon==2){ %><div class="price_plus">¥<%=oItem.dpicon.p%><i class="mod_plus_price"></i></div><% } %>
                    <%  if(oItem.dpicon && oItem.dpicon.icon==1){ %><div class="price_sams">¥<%=oItem.dpicon.p%><i class="mod_sams_price"></i></div><% } %>
                    <%  if(oItem.dpicon && oItem.dpicon.icon==3){ %><span class="single">¥<%=oItem.dpicon.p%></span><% } %>
                    <%  if(oItem.dpicon && oItem.dpicon.icon==4){ %><div class="single">单买价 ¥<%=oItem.jp%></div><% } %>
                    <%  if(oItem.dpicon && oItem.dpicon.icon==5){ %><div class="price_sams" style="color:#FF8800;">¥<%=oItem.dpicon.p%><span class="mod_fresh_price">新人价</span></div><% } %>
                </div>
                <%if(cartIcon){%>
                <div class="buy <%=cartIcon%>"></div>
                <% } %>
            </div>
        </div>
    </li>
